  "scripts": {
    "unit": "karma start test/unit/karma.conf.js --single-run",
    "unit-watch": "karma start test/unit/karma.conf.js",
    "build": "cross-env NODE_ENV=production npm run build:client && npm run build:server",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress",
    "build:client": "cross-env NODE_ENV=development webpack --config build/webpack.client.config.js --progress",
    "dev": "cross-env NODE_ENV=development webpack --config build/webpack.client.config.js --progress --color --watch --hide-modules |  webpack --config build/webpack.server.config.js --progress --color --watch --hide-modules",
    "e2e": "node test/e2e/runner.js"
  },
